   "source": [
    "from math import * #FUNCTION FOR EUCLIDEAN-DISTANCE\n",
    "def euclidean_distance(x,y):\n",
    "    return sqrt(sum(pow(a-b,2) for a,b in zip(x,y)))"
   ]
  },
  {
