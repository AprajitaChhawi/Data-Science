   "cell_type": "code",
   "execution_count": 64,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Chessblock_distance(A,B): #DEFINING CHESSBLOCK DISTANCE\n",
    "    result = n.sum([abs(a - b) for (a, b) in zip(A, B)]) \n",
    "    return result"
   ]
  },
