  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {},
   "outputs": [],
   "source": [
    "def cityblock_distance(A, B): #DEFINING CITYBLOCK DISTANCE\n",
    "    result = n.sum([abs(a - b) for (a, b) in zip(A, B)]) \n",
    "    return result"
   ]
  },
