   "source": [
    "from numpy import * #DEFINING CHEBYSHEV DISTANCE\n",
    "def chebyshev_distance(A, B): \n",
    "    result = ([abs(a-b).max() for (a, b) in zip(A, B)]) \n",
    "    return result"
   ]
  },
